{
  "linters": {
    "Python linting": {
      "type": "flake8",
      "include": "(\\.py$)",
      "exclude": "(migrations/)",
      "severity": {
        "E501": "disabled"
      }
    },
    "Javascript linting": {
      "type": "jshint",
      "include": "(\\.js$)",
      "exclude": "(vendors/)"
    }
  }
}
